{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "motif": {
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "loader": {
              ".svg": "text"
            },
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss",
                "./node_modules/@ng-select"
              ]
            },
            "outputPath": {
              "base": "dist"
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "preserveSymlinks": true,
            "assets": [
              "src/assets",
              "src/.htaccess",
              "src/favicon.ico",
              "src/silent-refresh.html",
              {
                "glob": "version.txt",
                "input": "src/generated",
                "output": "/"
              },
              {
                "glob": "oidc-client-ts.min.js",
                "input": "node_modules/oidc-client-ts/dist/browser",
                "output": "/"
              },
              {
                "glob": "oidc-client-ts.min.js.map",
                "input": "node_modules/oidc-client-ts/dist/browser",
                "output": "/"
              },
              {
                "glob": "README.md",
                "input": ".",
                "output": "/"
              }
            ],
            "styles": [
              "src/scss/style.scss",
              "src/scss/app-loading.scss"
            ],
            "baseHref": "/",
            "allowedCommonJsDependencies": [
              "crypto-js",
              "decimal.js-light",
              "rollbar",
              "toastify-js"
            ],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "16mb",
                  "maximumError": "17mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "20kb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "vendor": true,
                "hidden": false
              },
              "namedChunks": true,
              "assets": [
                "src/assets",
                "src/.htaccess",
                "src/favicon.ico",
                "src/silent-refresh.html",
                {
                  "glob": "version.txt",
                  "input": "src/generated",
                  "output": "/"
                },
                {
                  "glob": "oidc-client-ts.min.js",
                  "input": "node_modules/oidc-client-ts/dist/browser",
                  "output": "/"
                },
                {
                  "glob": "oidc-client-ts.min.js.map",
                  "input": "node_modules/oidc-client-ts/dist/browser",
                  "output": "/"
                },
                {
                  "glob": "README.md",
                  "input": ".",
                  "output": "/"
                },
                {
                  "glob": "**/*",
                  "input": "dev_static",
                  "output": "/"
                // },
                // {
                //   "glob": "*",
                //   // "input": "../../highcharts-motif-extension/dist",
                //   "input": "../extensions/highcharts/dist",
                //   "output": "/extensions/highcharts/"
                // },
                // {
                //   "glob": "*",
                //   // "input": "../../ts-demo-motif-extension/dist",
                //   "input": "../extensions/ts-demo/dist",
                //   "output": "/extensions/ts-demo/"
                // // },
                // // {
                // //   "glob": "*",
                // //   "input": "../../website-embed-motif-extension/dist",
                // //   "output": "/extensions/website-embed/"
                }
              ]
            },
            "production-dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "motif:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "motif:build:production-dev"
            },
            "development": {
              "buildTarget": "motif:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "motif:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "styles": [
              "src/scss/style.scss",
              "src/scss/app-loading.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/generated/version.txt"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "motif-e2e": {
      "root": "e2e",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "motif:serve"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss",
      "changeDetection": "OnPush",
      "displayBlock": true,
      "skipTests": true,
      "type": "component"
    },
    "@schematics/angular:directive": {
      "prefix": "app",
      "type": "directive"
    },
    "@schematics/angular:service": {
      "skipTests": true,
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  },
  "cli": {
    "analytics": "b4912cb0-3bfd-49bf-9403-f852fd45df9f",
    "schematicCollections": ["angular-eslint"]
  }
}
