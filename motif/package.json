{
  "name": "motif",
  "private": true,
  "version": "3.1.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plxtra/motif.git",
    "directory": "motif"
  },
  "bugs": {
    "url": "https://github.com/plxtra/motif/issues"
  },
  "homepage": "https://plxtra.org",
  "author": {
    "name": "Plxtra contributors",
    "url": "https://plxtra.org"
  },
  "files": [
    "dist/**/*"
  ],
  "typings": "lib/public-api.d.ts",
  "scripts": {
    "link-core": "npm link @pbkware/js-utils @pbkware/ui-action revgrid @plxtra/motif-core",
    "ng": "ng",
    "clean:cache": "npx del-cli .angular/cache",
    "clean:lib": "npx del-cli lib",
    "copy:config:trading-retailer_production-dev:config": "npx cpy-cli src/config/config.trading-retailer_production-dev.json dev_static/config/ --flat --rename=config.json",
    "copy:config:trading-retailer_production-dev:branding": "npx cpy-cli branding/paritech/paritech-desktop-bar-left-icon.svg dev_static/config/branding/images --flat --rename=desktop-bar-left-icon.svg",
    "copy:config:trading-retailer_production-dev": "npm run copy:config:trading-retailer_production-dev:config && npm run copy:config:trading-retailer_production-dev:branding",
    "copy:config:xosp-dev:config": "npx cpy-cli src/config/config.xosp-dev.json dev_static/config/ --flat --rename=config.json",
    "copy:config:xosp-dev:branding": "npx cpy-cli src/branding/xosp/xosp-desktop-bar-left-icon.svg dev_static/config/branding/images --flat --rename=desktop-bar-left-icon.svg",
    "copy:config:xosp-dev": "npm run copy:config:xosp-dev:config && npm run copy:config:xosp-dev:branding",
    "start:trading-retailer_production-dev": "npm run copy:config:trading-retailer_production-dev && ng serve",
    "start:xosp-dev": "npm run copy:config:xosp-dev && ng serve",
    "startSourceMaps": "ng serve --vendorSourceMap",
    "build:dev": "ng build -c=development",
    "build:prod": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build",
    "build:docker": "ng build",
    "test": "ng test",
    "lint": "ng lint motif",
    "lib": "npm run clean:lib && npx tsc -p ./tsconfig.app.json --declaration --declarationMap --emitDeclarationOnly --declarationDir ./lib",
    "rollup": "npx api-extractor run --local --verbose",
    "api": "npm run lib && npm run rollup"
  },
  "dependencies": {
    "@angular/animations": "^19.2.9",
    "@angular/common": "^19.2.9",
    "@angular/compiler": "^19.2.9",
    "@angular/core": "^19.2.9",
    "@angular/forms": "^19.2.9",
    "@angular/platform-browser": "^19.2.9",
    "@angular/platform-browser-dynamic": "^19.2.9",
    "@angular/router": "^19.2.9",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@ctrl/tinycolor": "^4.0.3",
    "@jaames/iro": "^5.5.1",
    "@ng-select/ng-select": "^14.7.0",
    "@pbkware/js-utils": "^0.8.2",
    "@pbkware/ui-action": "^0.9.2",
    "@plxtra/motif-core": "^0.9.5",
    "@types/toastify-js": "^1.12.3",
    "angular-split": "^19.0.0",
    "angular-svg-icon": "^19.1.1",
    "codemirror": "^6.0.1",
    "decimal.js-light": "^2.5.1",
    "file-saver": "^2.0.5",
    "golden-layout": "^2.6.0",
    "nanoid": "^5.1.5",
    "revgrid": "^0.9.3",
    "rxjs": "^7.8.2",
    "toastify-js": "^1.12.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular-eslint/builder": "^19.6.0",
    "@angular/cli": "^19.2.9",
    "@angular/compiler-cli": "^19.2.9",
    "@angular/language-service": "^19.2.9",
    "@eslint/js": "^9.28.0",
    "@microsoft/api-extractor": "^7.52.5",
    "angular-eslint": "19.7.0",
    "cpy-cli": "^5.0.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.28.0",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import-x": "^4.15.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-spec-reporter": "^7.0.0",
    "oidc-client-ts": "^3.2.0",
    "rollbar": "^2.26.4",
    "sass": "^1.87.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  }
}
