    "clean:lib": "if (Test-Path lib) { Remove-Item -Path lib -Recurse -Force }",
    "start:xosp-dev": "./PSScripts/Invoke-XospNgServe.ps1",
    "startSourceMaps": "ng serve --vendorSourceMap",
    "build:dev": "ng build -c=development",
    "build:prod": "ng build",
    "build:docker": "ng build",
    "lint": "ng lint motif",
    "lib": "npm run clean:lib && npx tsc -p ./tsconfig.app.json --declaration --declarationMap --emitDeclarationOnly --declarationDir ./lib",
    "create": "Docker-Create.ps1",
    "params": "Docker-Params.ps1",
    "publish": "Docker-Publish.ps1"
  },
